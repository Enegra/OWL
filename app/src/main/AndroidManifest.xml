<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.agnie.owl">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OWLMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dictionary"
            android:label="@string/dictionary_title" />
        <activity
            android:name=".DictionaryItemDetail"
            android:label="@string/dictionary_detail_title" />
        <activity
            android:name=".Favourites"
            android:label="@string/favourites_title" />
        <activity
            android:name=".Lessons"
            android:label="@string/lessons_title" />
        <activity android:name=".LessonDetail"></activity>
    </application>

</manifest>