<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.agnie.owl">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OWLMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dictionary" />
        <activity
            android:name=".DictionaryItemDetail" />
        <activity
            android:name=".Favourites" />
        <activity
            android:name=".Lessons" />
        <activity android:name=".LessonDetail" />
        <activity
            android:name=".Tests" />
        <activity android:name=".TestDetail" />
        <activity android:name=".Settings"></activity>
    </application>

</manifest>